        <attr name="image" format="reference"/>
        <attr name="text" format="reference|string"/>
    </declare-styleable>
    
    <!-- 夜间模式 属性定义 -->
    <attr name="nd_background" format="reference|string"/>
    <attr name="nd_textcolor" format="reference|string"/> 
    
    
    
    <declare-styleable name="round_imageview_attrs">
        <attr name="border_width" format="dimension"/>  <!-- 边框宽度 -->
        <attr name="border_inside_color" format="color"/><!-- 内边框颜色 -->
